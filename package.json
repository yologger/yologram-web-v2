{
  "name": "yologram-web-v2",
  "version": "0.0.2",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "vite --mode development --open",
    "start:staging": "vite --mode staging --open",
    "start:prod": "vite --mode production --open",
    "build:dev": "tsc -b && vite build --mode development",
    "build:staging": "tsc -b && vite build --mode staging",
    "build:prod": "tsc -b && vite build --mode production",
    "lint": "eslint . --ext ts,tsx,js,jsx",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.6.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@eslint/js": "^9.30.1",
    "vite": "^7.0.4",
    "typescript": "~5.8.3",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-config-prettier": "^10.1.5",
    "typescript-eslint": "^8.35.1",
    "prettier": "^3.6.2"
  }
}
